<Page x:Class="prototip.Auto"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:prototip"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Auto">

    <Grid>
        <StackPanel>
            <TextBlock Text="Введите логин и пароль"></TextBlock>
            <TextBox Name="log" Text="admin"></TextBox>
            <PasswordBox Name="pas" Password="admin"></PasswordBox>
            <Button x:Name="btnauto" Click="btnauto_Click" Content="Авторизация" Width="100" Height="25"></Button>
        </StackPanel>
    </Grid>
</Page>
