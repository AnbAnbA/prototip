<Page x:Class="prototip.Auto"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:prototip"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Auto">

    <Grid>
        <StackPanel>
            <TextBlock Text="Введите логин и пароль"></TextBlock>
            <TextBox Name="log" Text="admin"></TextBox>
            <PasswordBox Name="pas" Password="admin"></PasswordBox>
            <Button Margin="5" x:Name="btnauto" Click="btnauto_Click" Content="Авторизация" Width="100" Height="25" IsEnabled="False"></Button>
            <Label Name="lb" Content="Повторный код можно получить через:" Visibility="Collapsed" ></Label>
            <Label x:Name="time"  Visibility="Collapsed"></Label>
            <Button Margin="5" Name="btncode" Width="150" Height="25" Click="btncode_Click" Visibility="Collapsed" IsEnabled="False" Content="Получить код повторно"></Button>
        </StackPanel>
    </Grid>
</Page>
