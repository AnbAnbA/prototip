        xmlns:local="clr-namespace:prototip"
        mc:Ignorable="d"
        Title="WindowCaptcha" Height="250" Width="500">
    <Grid>
        <StackPanel>
            <Canvas x:Name="canvas" Width="250" Height="120" Margin="15">
                <Canvas x:Name="canvas1" Margin="220,0,0,0" Width="30" Height="30">
                    
                    
                </Canvas>
            </Canvas>
            <StackPanel>
                <TextBox Name="captchacode" Width="120"></TextBox>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
